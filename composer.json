{
  "name": "swisnl/json-api-server",
  "type": "library",
  "description": "Set up a JSON API in Laravel in just a few minutes.",
  "keywords": [
    "swisnl",
    "laravel",
    "laravel-api",
    "json-api",
    "json-api-server"
  ],
  "homepage": "http://www.swis.nl",
  "license": "MIT",
  "authors": [
    {
      "name": "Arnaud van Zandwijk",
      "email": "arnaud@swis.nl",
      "homepage": "https://www.swis.nl",
      "role": "Developer"
    },
    {
      "name": "BjÃ¶rn Brala",
      "email": "bjorn@swis.nl",
      "homepage": "https://www.swis.nl",
      "role": "Developer"
    },
    {
      "name": "Dylan de Wit",
      "email": "ddewit@swis.nl",
      "homepage": "https://www.swis.nl",
      "role": "Developer"
    },
    {
      "name": "Dani Tulp",
      "email": "dtulp@swis.nl",
      "homepage": "https://www.swis.nl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "dimsav/laravel-translatable": "^8.0|^9.0",
    "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*|6.1.*"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "friendsofphp/php-cs-fixer": "^2.4",
    "mockery/mockery": "^1.0",
    "graham-campbell/testbench": "^4.0|^5.1",
    "phpunit/php-code-coverage": "^5.3|^6.0",
    "phpunit/phpunit": "^7.0|^6.1",
    "spatie/phpunit-watcher": "^1.3",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Swis\\JsonApi\\Server\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-watch": "vendor/bin/phpunit-watcher watch",
    "check-style": "vendor/bin/php-cs-fixer fix --dry-run -vvv",
    "fix-style": "vendor/bin/php-cs-fixer fix -vvv"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Swis\\JsonApi\\Server\\Providers\\LaravelApiServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
